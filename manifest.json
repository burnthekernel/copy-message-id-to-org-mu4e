{
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "copy-message-id-to-org-mu4e@burnthekernel",
      "strict_min_version": "68.7.0"
    }
  },
  "name": "[[org-link mu4e]]",
  "description": "Adds a button to the message view toolbar to copy the message ID to the clipboard as an org mode link to mu4e",
  "permissions": ["storage", "messagesRead", "clipboardWrite"],
  "message_display_action": {
    "default_title": "[[org-link mu4e]]",
    "default_icon": "icons/button.png",
    "default_popup": "popup/popup.html"
  },
  "version": "1.0.2"
}
